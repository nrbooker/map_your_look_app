<div class="page-header">
  <h3>Eye Item #<%= @eye_item.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Makeup Type</dt>
      <dd><%= @eye_item.store %></dd>

      <dt>Brand</dt>
      <dd><%= @eye_item.brand %></dd>

      <dt>Color</dt>
      <dd><%= @eye_item.color %></dd>

      <dt>Eye</dt>
      <dd><%= @eye_item.eye_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/eye_items" class="btn btn-primary">
        Back
      </a>
      <a href="/eye_items/<%= @eye_item.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_eye_item/<%= @eye_item.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
