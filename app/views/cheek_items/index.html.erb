<div class="page-header">
  <h1>
    Cheeky Catalog
    <a href="/cheek_items/new" class="btn btn-lg btn-success">Add Warmth</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Makeup Type</th>
        <th>Brand</th>
        <th>Color</th>
        <th>Cheek</th>
        <th>Actions</th>
      </tr>

      <% @cheek_items.each do |cheek_item| %>
      <tr>
        <td><%= cheek_item.store %></td>
        <td><%= cheek_item.brand %></td>
        <td><%= cheek_item.color %></td>
        <td><%= cheek_item.cheek_id %></td>
        <td>
          <a href="/cheek_items/<%= cheek_item.id %>" class="btn btn-primary">Show</a>
          <a href="/cheek_items/<%= cheek_item.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_cheek_item/<%= cheek_item.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
